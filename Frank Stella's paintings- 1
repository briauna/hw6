function setup() { 
  createCanvas(400, 400);
} 
function draw() { 
	colorMode(HSB);
h= random(320);

	var y4=10;
while (y4<320) {
 fill(h,100,100);
ellipse(200,150,y4,y4);
  y4=y4+40;
}
 var y=10;
while (y<100) {
  noFill();
ellipse(170,170,y,y);
  y=y+20;
}
	 var y2=10;
while (y2<150) {
  noFill();
ellipse(100,100,y2,y2);
  y2=y2+20;
}
	 var y3=10;
while (y3<120) {
  noFill();
ellipse(250,150,y3,y3);
  y3=y3+20;
}

}
